FoamFile
{
    version     2.0;
    format      ascii;
    class       volTensorField;
    location    "0";
    object      Df;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 -2 0 0 0 0 0];

internalField   uniform (6e+10 0 0 0 6e+10 0 0 0 6e+10);

boundaryField
{
    wall
    {
        type            calculated;
        value           uniform (0 0 0 0 0 0 0 0 0);
    }
    inlet
    {
        type            calculated;
        value           uniform (0 0 0 0 0 0 0 0 0);
    }
    outlet
    {
        type            calculated;
        value           uniform (0 0 0 0 0 0 0 0 0);
    }
}


